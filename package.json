{
  "name": "magic-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "cors": "^2.8.5",
    "jest-localstorage-mock": "^2.4.26",
    "json-server": "^0.17.1",
    "lowdb": "^5.0.5",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.3",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-css": "nodemon -e scss -x \"sass --watch src/sass/index.scss:src/styles/index.css\"",
    "test": "react-scripts test test --coverage --watchAll",
    "eject": "react-scripts eject",
    "test:all": "react-scripts test --watchAll --collect-coverage",
    "test:prod": "react-scripts test --watchAll --collect-coverage --watchAll=false",
    "lint": "eslint .",
    "start:dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "json-server": "node server.js",
    "server": "json-server --watch db.json --port 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "@babel/plugin-syntax-jsx"
    ],
    "rules": {
      "testing-library/no-render-in-setup": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/reportWebVitals.ts",
      "<rootDir>/src/index.ts",
      "<rootDir>/src/tools/*"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint-config-prettier": "^8.5.0",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "^7.0.3"
  }
}
